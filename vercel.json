{
    "version": 2,
    "builds": [
        {
            "src": "src/app.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "src/**/*.ts"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/app.ts"
        }
    ],
    "functions": {
        "src/app.ts": {
            "maxDuration": 60,
            "maxLambdaSize": "50mb"
        }
    }
}